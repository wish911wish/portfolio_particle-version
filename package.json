{
  "name": "vue-todo",
  "version": "0.0.0",
  "description": "TODO list of vue",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "css/sass": "node-sass src/assets/sass/style.scss -o dist/assets/css --output-style expanded --source-map dist/assets/css",
    "css/postcss": "postcss dist/assets/css/style.css -o dist/assets/css/style.min.css",
    "clean/css": "rimraf dist/css && mkdir -p dist/css",
    "css": "npm run clean/css && npm-run-all -p css/*",
    "js/rollup": "rollup src/assets/js/index.js -c=rollup.config.js",
    "js/uglifyjs": "uglifyjs dist/assets/js/index.js -o dist/assets/js/index.min.js",
    "clean/js": "rimraf dist/assets/js && mkdir -p dist/assets/js",
    "js": "npm run clean/js && npm-run-all -s js/*",
    "images/imagemin": "node imagemin.js",
    "clean/images": "rimraf dist/assets/images && midir -p dist/assets/images",
    "images": "npm run clean/images && npm-run-all -s images/*",
    "watch/server": "browser-sync start -s dist -f 'src, dist, dist/assets/js/*, dist/assets/css/*, **/*.html, !node_modules/**/*'",
    "watch/css": "watch 'npm run css' ./src/assets/sass",
    "watch/js": "watch 'npm run js' ./src/assets/js",
    "watch/images": "onchange 'src/assets/images' -e '**/*.DS_Store' -- npm run images",
    "html/plain": "cpx 'src/assets/html/**/*.html' 'dist/'",
    "html/ejscli": "ejs-cli --base-dir src/assets/ejs '**/*.ejs' --out dist",
    "html": "npm-run-all html/*",
    "watch/ejs": "watch 'npm run html' ./src/assets/ejs",
    "watch/html": "watch 'npm run html' ./src/assets/html",
    "watch": "npm-run-all -p watch/*"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "browser-sync": "^2.26.7",
    "cpx": "^1.5.0",
    "ejs-cli": "^2.1.0",
    "imagemin": "^7.0.0",
    "imagemin-gifsicle": "^6.0.1",
    "imagemin-keep-folder": "^5.3.2",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-pngquant": "^8.0.0",
    "imagemin-svgo": "^7.0.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "onchange": "^6.0.0",
    "rollup": "^1.20.3",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "uglify-js": "^3.6.0",
    "watch": "^1.0.2"
  },
  "dependencies": {
    "autoprefixer": "^9.6.1",
    "cssnano": "^4.1.10",
    "postcss-cli": "^6.1.3"
  }
}
